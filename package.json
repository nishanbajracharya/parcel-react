{
  "name": "parcel-react",
  "version": "1.0.0",
  "description": "A look at setting up a react project using parcel bundler",
  "main": "./src/js/index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint src",
    "test:update": "jest -u",
    "prestart": "rimraf dist",
    "lint:fix": "eslint src --fix",
    "start": "parcel ./src/index.html -p 3000",
    "build:production": "rimraf dist && parcel build ./src/index.html -d dist/ --no-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nishanbajracharya/parcel-react.git"
  },
  "keywords": [
    "react",
    "parcel",
    "parcel-bundler"
  ],
  "author": "Nishan Bajracharya <nisbaj11@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nishanbajracharya/parcel-react/issues"
  },
  "homepage": "https://github.com/nishanbajracharya/parcel-react#readme",
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.15.0",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^22.1.1",
    "parcel-bundler": "^1.4.1",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "normalize.css": "^7.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
